{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "Simplified React Kickstarter",
  "main": "build/components.js",
  "style": "build/styles.css",
  "config": {
    "integration": "frontend/**/*.integration.test.+(js|jsx)",
    "test": "frontend/**/!(*.integration).test.+(js|jsx)"
  },
  "scripts": {
    "build:dll": "cross-env NODE_ENV=development npm run webpack -- --config webpack/config.dll.babel.js",
    "build": "npm run webpack -- --config webpack/config.prod.babel.js",
    "clean": "rimraf build coverage",
    "lint:js": "eslint . --ext js,jsx --ignore-path .gitignore",
    "lint:style": "stylelint \"frontend/**/*.@(less|css)\"",
    "lint": "npm run lint:js && npm run lint:style",
    "mocha": "NODE_ENV=test mocha --require babel-register --require \"./frontend/test\" --opts \"./frontend/test/mocha.opts\"",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "start": "nodemon -r \"babel-core/register\" dev",
    "test": "npm run test:integration && npm run test:coverage",
    "test:coverage": "cross-env NODE_ENV=test BABEL_ENV=test nyc mocha $npm_package_config_test --require \"./frontend/test\" --opts \"./frontend/test/mocha.opts\"",
    "test:integration": "npm run mocha $npm_package_config_integration",
    "test:unit": "npm run mocha $npm_package_config_test -- $@",
    "webpack": "cross-env NODE_ENV=${NODE_ENV:-production} webpack --progress --hide-modules"
  },
  "lint-staged": {
    "**/*.{js,json,css,jsx}": [
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ui/charlie-redux.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "webpack-dev-server",
    "postcss",
    "jest",
    "boilerplate",
    "hmr"
  ],
  "author": "Doug Farmer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "classnames": "^2.2.6",
    "classlist-polyfill": "^1.2.0",
    "core-js": "^2.6.1",
    "es6-error": "^4.1.1",
    "jquery": "~1.11.3",
    "history": "^4.7.2",
    "prop-types": "^15.5.8",
    "moment": "~2.18.1",
    "react": "^0.4.7",
    "react-dom": "^0.14.7",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.4",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.0",
    "redux-form": "^8.0.4",
    "redux-responsive": "^4.3.8",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.1",
    "reselect": "^4.0.0",
    "sha.js": "^2.4.11"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap-less": "^3.3.8",
    "chai": "^4.2.0",
    "chai-enzyme": "^0.8.0",
    "clean-webpack-plugin": "^1.0.0",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.8.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.0",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
