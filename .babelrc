{
    "presets": [
        ["@babel/preset-env", "@babel/preset-react"]
    ],
    "plugins": [
        "transform-react-jsx",
        "jsx-control-statements",
        "transform-class-properties"
    ],
    "env": {
        "test": {
            "plugins": ["istanbul"]
        },
        "production": {
            "plugins": [
                "jsx-control-statements",
                "transform-react-constant-elements",
                "transform-react-remove-prop-types"
            ]
        }
    }
}